<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title>Calender</title>
<link href='http://fonts.googleapis.com/css?family=Lato:300,400,700' rel='stylesheet' type='text/css'>
<link rel="stylesheet" href="/public/assets/styles.css" type="text/css" media="screen" charset="utf-8">

</head>
<div>
    <%- include('partials/navbar') %>
</div>


<body>
    <div class="main-content">
        <h1>
            Calender
        </h1>
        <form method="POST" id="calenderForm">
            <p>
                <input id="calender" name="calender" placeholder="Paste Calender URL" type="text">
            </p>
            <div class="text-end">
                <button id="cancelButton" type="reset" class="btn btn-warning"><i class="fas fa-undo fa-lg"></i>
                    Reset</button>
                <button id="findButton" type="submit" class="btn btn-primary"><i class="fas fa-undo fa-lg"></i> Find
                    Calender</button>
            </div>
        </form>
    </div>

    <div>
        <%- include('partials/footer') %>
    </div>
</body>